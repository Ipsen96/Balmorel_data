*Same hotwater demand, but 2050 space heating demand has the HRE2050 reduction ratio, equally distributed to years in between                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
INDHEAT_DH(Y,INDHEAT_AAA,'HOTWATER')              =                                                       INDHEAT_DH('2020',INDHEAT_AAA,'HOTWATER');                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
INDHEAT_DH('2030',INDHEAT_AAA,'SPACEHEAT')              =                                                 INDHEAT_DH('2020',INDHEAT_AAA,'SPACEHEAT')+(INDHEAT_DH('2050',INDHEAT_AAA,'SPACEHEAT')-INDHEAT_DH('2020',INDHEAT_AAA,'SPACEHEAT'))/3;                                                                                                                                                                                                                                                                                                                                                                                                                                           
INDHEAT_DH('2040',INDHEAT_AAA,'SPACEHEAT')              =                                                 INDHEAT_DH('2020',INDHEAT_AAA,'SPACEHEAT')+(INDHEAT_DH('2050',INDHEAT_AAA,'SPACEHEAT')-INDHEAT_DH('2020',INDHEAT_AAA,'SPACEHEAT'))/3*2;                                                                                                                                                                                                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
