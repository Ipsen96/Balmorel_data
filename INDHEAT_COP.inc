
PARAMETER INDHEAT_COP(INDHEAT_AAA,INDHEAT_GGG,SSS,TTT) 'COP of heat pump technologies' ;

PARAMETER INDHEAT_EFFICIENCY(INDHEAT_GGG) 'Multiplication factor to reach the given COP from GDATA';
*Note that I on purpose used the COP from the earlier technology as reference as we do not assume it can be used for hot water demand
*I have further multiplied with the increase in COP as from 2010 to 2020
INDHEAT_EFFICIENCY('IDVHG_HPA2A_2010_OFH') = INDHEAT_GDATA('IDVHG_HPA2A_2010_OFH','GDFE')/((272+20)/(20-7));
INDHEAT_EFFICIENCY('IDVHG_HPA2A_2020_OFH') = INDHEAT_GDATA('IDVHG_HPA2A_2020_OFH','GDFE')/((272+20)/(20-7));
INDHEAT_EFFICIENCY('IDVHG_HPA2A_2030_OFH') = INDHEAT_GDATA('IDVHG_HPA2A_2020_OFH','GDFE')/((272+20)/(20-7))*(1+(INDHEAT_GDATA('IDVHG_HPA2A_2020_OFH','GDFE')-INDHEAT_GDATA('IDVHG_HPA2A_2010_OFH','GDFE'))/INDHEAT_GDATA('IDVHG_HPA2A_2010_OFH','GDFE'));
INDHEAT_EFFICIENCY('IDVHG_HPA2A_2040_OFH') = INDHEAT_EFFICIENCY('IDVHG_HPA2A_2030_OFH')*(1+(INDHEAT_GDATA('IDVHG_HPA2A_2020_OFH','GDFE')-INDHEAT_GDATA('IDVHG_HPA2A_2010_OFH','GDFE'))/INDHEAT_GDATA('IDVHG_HPA2A_2010_OFH','GDFE'));
INDHEAT_EFFICIENCY('IDVHG_HPA2A_2050_OFH') = INDHEAT_EFFICIENCY('IDVHG_HPA2A_2040_OFH')*(1+(INDHEAT_GDATA('IDVHG_HPA2A_2020_OFH','GDFE')-INDHEAT_GDATA('IDVHG_HPA2A_2010_OFH','GDFE'))/INDHEAT_GDATA('IDVHG_HPA2A_2010_OFH','GDFE'));

INDHEAT_EFFICIENCY('IDVHG_HPA2W_2010_OFH') = INDHEAT_GDATA('IDVHG_HPA2W_2010_OFH','GDFE')/((272+55)/(55-7));
INDHEAT_EFFICIENCY('IDVHG_HPA2W_2020_OFH') = INDHEAT_GDATA('IDVHG_HPA2W_2020_OFH','GDFE')/((272+55)/(55-7));
INDHEAT_EFFICIENCY('IDVHG_HPA2W_2030_OFH') = INDHEAT_GDATA('IDVHG_HPA2W_2030_OFH','GDFE')/((272+55)/(55-7));
INDHEAT_EFFICIENCY('IDVHG_HPA2W_2040_OFH') = INDHEAT_GDATA('IDVHG_HPA2W_2040_OFH','GDFE')/((272+55)/(55-7));
INDHEAT_EFFICIENCY('IDVHG_HPA2W_2050_OFH') = INDHEAT_GDATA('IDVHG_HPA2W_2050_OFH','GDFE')/((272+55)/(55-7));

INDHEAT_EFFICIENCY('IDVHG_HPA2W_2010_APART') = INDHEAT_GDATA('IDVHG_HPA2W_2010_APART','GDFE')/((272+55)/(55-7));
INDHEAT_EFFICIENCY('IDVHG_HPA2W_2020_APART') = INDHEAT_GDATA('IDVHG_HPA2W_2020_APART','GDFE')/((272+55)/(55-7));
INDHEAT_EFFICIENCY('IDVHG_HPA2W_2030_APART') = INDHEAT_GDATA('IDVHG_HPA2W_2030_APART','GDFE')/((272+55)/(55-7));
INDHEAT_EFFICIENCY('IDVHG_HPA2W_2040_APART') = INDHEAT_GDATA('IDVHG_HPA2W_2040_APART','GDFE')/((272+55)/(55-7));
INDHEAT_EFFICIENCY('IDVHG_HPA2W_2050_APART') = INDHEAT_GDATA('IDVHG_HPA2W_2050_APART','GDFE')/((272+55)/(55-7));

*Assumption on indoor temperature = 20 degrees celcius - efficiency as above
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2A_2010_OFH',S,T)= (272+20)/(20-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T))$(SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)) <10) - 10$(SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)) GE 10))*INDHEAT_EFFICIENCY('IDVHG_HPA2A_2010_OFH');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2A_2020_OFH',S,T)= (272+20)/(20-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T))$(SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)) <10) - 10$(SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)) GE 10))*INDHEAT_EFFICIENCY('IDVHG_HPA2A_2020_OFH');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2A_2040_OFH',S,T)= (272+20)/(20-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T))$(SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)) <10) - 10$(SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)) GE 10))*INDHEAT_EFFICIENCY('IDVHG_HPA2A_2030_OFH');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2A_2030_OFH',S,T)= (272+20)/(20-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T))$(SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)) <10) - 10$(SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)) GE 10))*INDHEAT_EFFICIENCY('IDVHG_HPA2A_2040_OFH');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2A_2050_OFH',S,T)= (272+20)/(20-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T))$(SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)) <10) - 10$(SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)) GE 10))*INDHEAT_EFFICIENCY('IDVHG_HPA2A_2050_OFH');

*Assumption on temperature = 55 degrees celcius - hot water - efficiency as above
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2010_OFH',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*INDHEAT_EFFICIENCY('IDVHG_HPA2W_2010_OFH');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2020_OFH',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*INDHEAT_EFFICIENCY('IDVHG_HPA2W_2020_OFH');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2030_OFH',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*INDHEAT_EFFICIENCY('IDVHG_HPA2W_2030_OFH');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2040_OFH',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*INDHEAT_EFFICIENCY('IDVHG_HPA2W_2040_OFH');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2050_OFH',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*INDHEAT_EFFICIENCY('IDVHG_HPA2W_2050_OFH');

*Assumption on temperature = 55 degrees celcius - hot water - efficiency as above
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2010_APART',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*INDHEAT_EFFICIENCY('IDVHG_HPA2W_2010_APART');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2020_APART',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*INDHEAT_EFFICIENCY('IDVHG_HPA2W_2020_APART');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2030_APART',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*INDHEAT_EFFICIENCY('IDVHG_HPA2W_2030_APART');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2040_APART',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*INDHEAT_EFFICIENCY('IDVHG_HPA2W_2040_APART');
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2050_APART',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*INDHEAT_EFFICIENCY('IDVHG_HPA2W_2050_APART');


