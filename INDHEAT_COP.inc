INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2A_2040_OFH',S,T)= (272+20)/(20-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*0.23;
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2A_2050_OFH',S,T)= (272+20)/(20-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*0.23;

*Assumption on temperature = 55 degrees celcius - hot water - efficiency 50%
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2010_OFH',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*0.5;
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2020_OFH',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*0.5;
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2030_OFH',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*0.5;
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2040_OFH',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*0.5;
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2050_OFH',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*0.5;

*Assumption on temperature = 55 degrees celcius - hot water - efficiency 50%
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2010_APART',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*0.5;
INDHEAT_COP(INDHEAT_AAA,'IDVHG_HPA2W_2020_APART',S,T)= (272+55)/(55-SUM(RRR$RRRAAA(RRR,INDHEAT_AAA),TEMP_VAR_T(RRR,S,T)))*0.5;
