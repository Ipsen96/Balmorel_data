INDHEAT_COP(INDHEAT_AAA,INDHEAT_G,S,T)$((INDHEAT_GDATA(INDHEAT_G,'GDTYPE') EQ GETOH AND INDHEAT_GDATA(INDHEAT_G,'GDFE')>1.5) AND (INDHEAT_COP(INDHEAT_AAA,INDHEAT_G,S,T) LE 0)) = 100;
INDHEAT_COP(INDHEAT_AAA,INDHEAT_G,S,T)=ROUND(INDHEAT_COP(INDHEAT_AAA,INDHEAT_G,S,T),4);